  <!-- ELEMENTS -->
  
  <!-- Application Layer -->
  <element identifier="app-01" xsi:type="ApplicationComponent">
    <name>ArchiMate Renderer Core</name>
    <documentation>Main renderer class for converting ArchiMate XML to SVG</documentation>
  </element>
  
  <element identifier="app-02" xsi:type="ApplicationComponent">
    <name>CLI Interface</name>
    <documentation>Command line interface for the renderer</documentation>
  </element>
  
  <element identifier="app-03" xsi:type="ApplicationComponent">
    <name>SVG Generator</name>
    <documentation>Utility for generating SVG elements for ArchiMate diagrams</documentation>
  </element>
  
  <element identifier="app-04" xsi:type="ApplicationComponent">
    <name>Shape Registry</name>
    <documentation>Registry for element shapes and relationship styles</documentation>
  </element>
  
  <element identifier="app-05" xsi:type="ApplicationComponent">
    <name>XML Parser</name>
    <documentation>Cross-platform XML parsing utility</documentation>
  </element>
  
  <element identifier="app-06" xsi:type="ApplicationComponent">
    <name>Text Wrapper</name>
    <documentation>Utility for wrapping and escaping text in SVG</documentation>
  </element>
  
  <element identifier="app-07" xsi:type="ApplicationComponent">
    <name>Compound Element Detector</name>
    <documentation>Utility for detecting and processing compound elements</documentation>
  </element>
  
  <element identifier="app-08" xsi:type="ApplicationComponent">
    <name>Icon Renderer</name>
    <documentation>Utility for rendering ArchiMate element icons</documentation>
  </element>
  
  <element identifier="app-09" xsi:type="ApplicationComponent">
    <name>Shape Templates</name>
    <documentation>Templates for common ArchiMate shapes</documentation>
  </element>
  
  <!-- Application Services -->
  <element identifier="svc-01" xsi:type="ApplicationService">
